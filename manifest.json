{
	"manifest_version": 2,
  	"name": "DNA Tool",
  	"description": "This is amazon tool to fetch reviews and download a excel sheet.",
  	"version": "1.0",
  	"permissions": [
		  "tabs", "https://amz.rudraserver.com/*"
	 ],
  "browser_action": {
    "default_icon": "assets/images/icon-32.png",
    "default_popup": "popup.html",
